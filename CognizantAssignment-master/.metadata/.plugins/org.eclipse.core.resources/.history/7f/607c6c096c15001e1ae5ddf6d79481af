package JavaTrickyQuestion;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class CopyOfArrayList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List<Integer>list=Arrays.asList(12,23,43,12,54);
		System.out.println(list);
		
		
		
		List<Integer>multiply=Arrays.asList(1,2,3,4,5);
		
		Double max = multiply.stream().collect(Collectors.averagingInt(Integer::intValue));
		System.out.println(max);
	}

}
