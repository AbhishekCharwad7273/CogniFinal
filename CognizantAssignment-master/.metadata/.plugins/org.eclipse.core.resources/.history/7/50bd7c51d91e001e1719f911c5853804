import java.time.DayOfWeek;
import java.time.LocalDate;
import java.time.Month;
import java.time.Period;
import java.time.format.DateTimeFormatter;
import java.time.format.FormatStyle;

public class PrintWednesdayInMonth {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int year=31;
		
		LocalDate now=LocalDate.of(year, Month.JULY, 1);
		
		LocalDate wed=now.with(DayOfWeek.WEDNESDAY);

		do {
			System.out.println(wed.format(DateTimeFormatter.ofLocalizedDate(FormatStyle.FULL)));
			wed=wed.plus(Period.ofDays(7));
		}while(wed.getDayOfMonth()==year);

	}

}
