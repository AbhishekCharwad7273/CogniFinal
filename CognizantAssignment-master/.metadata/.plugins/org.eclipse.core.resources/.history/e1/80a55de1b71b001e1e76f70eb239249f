package JavaTrickyQuestion;

public class Demo {

	public static void main(String[] args) {
		
		int no=121;
		int temp=no;
		int rem;
		while(temp>0)
		{
			rem=no%10;
			temp=temp+rem;
			no=no/2;
		}
		if(temp==no)
		{
			System.out.println("integer are palindrome");
		}
		else
		{
			System.out.println("not palindrome");
		}
		}
	
	}

