package JavaTrickyQuestion;


public class Demo {

	public static void main(String[] args) {
		
	String str="abbssdbfjeejksjk";
		Map<Character, Long> stringCount = str.chars().mapToObj(c->(char)c).collect(Collectors.groupingBy(c->c,Collectors.counting()));
		System.out.println(stringCount);
		
	}
}
