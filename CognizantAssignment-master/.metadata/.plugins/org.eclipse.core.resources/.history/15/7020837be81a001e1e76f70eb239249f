package JavaTrickyQuestion;

public class Demo {

	public static void main(String[] args) {

		String str[] = "I Love Java".split(" ");
		String rStr = "";

		for (int i = str.length - 1; i >= 0; i--) {
			rStr = rStr + str[i] + " ";

		}

		System.out.println(rStr.substring(0, rStr.length() - 1));

	}

}
