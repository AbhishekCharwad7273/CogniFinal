package JavaTrickyQuestion;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

//find vowelsOfString
//find duplication of string

public class OwelsOfString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		List<String>l=Arrays.asList("hello hello");
		Map<String, Long> duplicateCharcter = l.stream().collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
		System.out.println(duplicateCharcter);
	}

}
