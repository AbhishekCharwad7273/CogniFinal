package JavaTrickyQuestion;

public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int no=3;
		int isPrime = 0;
		
		for(int i=1;i<=no;i++)
		{
			isPrime=0;
			
			for(int j=2;j<i/2;j++)
			{
				if(i%j==0)
				{
					
					isPrime=1;
					break;
				}
			}
			
			
		}
		
		if(isPrime==0)
		{
			System.out.println(no+" Given no is Prime Number");
		}
		
	}

}
