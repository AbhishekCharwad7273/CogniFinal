package JavaTrickyQuestion;



public class Demo {

	public static void main(String[] args) {
		
		
		int arr1[]= {1,2,3,4,5,6};
		
		for(int i=0;i<arr1.length;i++)
		{
			System.out.print(arr1[i]);
		}
		System.out.println();
		
		for(int i=arr1.length-1;i>0;i--)
		{
			System.out.print(arr1[i]);
		}
			
	}
}
