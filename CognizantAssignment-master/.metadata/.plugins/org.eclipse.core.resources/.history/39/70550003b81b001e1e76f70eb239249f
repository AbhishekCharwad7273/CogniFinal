package JavaTrickyQuestion;

public class Demo {

	public static void main(String[] args) {
		
		int no=121;
		int temp=no;
		int rem;
		int sum=0;
		while(temp>0)
		{
			rem=no%10;
			sum=(sum*10)+rem;
			no=no/10;
		}
		if(temp==sum)
		{
			System.out.println("integer are palindrome");
		}
		else
		{
			System.out.println("not palindrome");
		}
		}
	
	}

