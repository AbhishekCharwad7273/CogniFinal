package JavaTrickyQuestion;

public class Demo {

	public static void main(String[] args) {
	
		
		String str="abaa";
		String chkPalindrome="";
		
		for(int i=str.length()-1;i>=0;i--)
		{
			chkPalindrome=chkPalindrome+str.charAt(i);
		}
		
		if(str.toLowerCase().equals(chkPalindrome.toLowerCase()))
		{
			System.out.println("String are palindrome");
		}
		else
		{
			System.out.println("String are Not Palindrome");
		}
	}

}
